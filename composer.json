    "authors": [
        {
            "name": "Rodrigo Manara",
            "email": "me@rodrigomanara.co.uk",
            "homepage": "http://rodrigomanara.co.uk"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/finder": "^3.1",
        "symfony/filesystem": "^3.1"
    },
    "autoload": {
        "psr-4": {
